<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE="true"
    STARTABLE="true"
	PARTITIONING="AUTO"
    >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Semantic Web</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
      <TABLE READONLY="true">
        <COLUMN NAME="SparqlResults" TYPE="id_String" READONLY="true" CUSTOM="true" />
      </TABLE>
    </PARAMETER>
    
    <PARAMETER NAME="VIRTUOSO_ENDPOINT" FIELD="TEXT" SHOW_IF="TRIPLESTORE_TYPE == 'VIRTUOSO'" NUM_ROW="5">
      <DEFAULT>"http://pubmed.bio2rdf.org/sparql"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="EBI_ENDPOINT" FIELD="TEXT" READONLY="true" SHOW_IF="TRIPLESTORE_TYPE == 'EBI'" NUM_ROW="5">
      <DEFAULT>"www.ebi.ac.uk/rdf/services/reactome/sparql"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="LINKEDLIFE_ENDPOINT" FIELD="TEXT" SHOW_IF="TRIPLESTORE_TYPE == 'LINKEDLIFEDATA'" NUM_ROW="5">
      <DEFAULT>"http://linkedlifedata.com/sparql"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="UNIPROT_ENDPOINT" FIELD="TEXT" READONLY="true" SHOW_IF="TRIPLESTORE_TYPE == 'UNIPROT'" NUM_ROW="5">
      <DEFAULT>"http://beta.sparql.uniprot.org/"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="TRIPLESTORE_TYPE" FIELD="CLOSED_LIST" NUM_ROW="10">
      <ITEMS DEFAULT="VIRTUOSO">
        <ITEM NAME="VIRTUOSO" VALUE="virtuoso"/>
        <ITEM NAME="EBI" VALUE="ebi"/>
        <ITEM NAME="UNIPROT" VALUE="uniprot"/>
        <ITEM NAME="LINKEDLIFEDATA" VALUE="linkedlifedata"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="FORMAT_VIRTUOSO" FIELD="CLOSED_LIST" SHOW_IF="TRIPLESTORE_TYPE == 'VIRTUOSO'" NUM_ROW="15">
      <ITEMS DEFAULT="RDFXML">
        <ITEM NAME="RDFXML" VALUE="application%2Frdf%2Bxml"/>
        <ITEM NAME="N3" VALUE="text%2Frdf%2Bn3"/>
        <ITEM NAME="NT" VALUE="text%2Fplain"/>
        <ITEM NAME="CSV" VALUE="text%2Fcsv"/>
        <ITEM NAME="JSON" VALUE="application%2Fsparql-results%2Bjson"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="FORMAT_EBI" FIELD="CLOSED_LIST" SHOW_IF="TRIPLESTORE_TYPE == 'EBI'" NUM_ROW="15">
      <ITEMS DEFAULT="RDFXML">
        <ITEM NAME="RDFXML" VALUE="RDF/XML"/>
        <ITEM NAME="N3" VALUE="N3"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="FORMAT_LINKEDLIFE" FIELD="CLOSED_LIST" SHOW_IF="TRIPLESTORE_TYPE == 'LINKEDLIFEDATA'" NUM_ROW="15">
      <ITEMS DEFAULT="XML">
        <ITEM NAME="XML" VALUE="xml"/>
        <ITEM NAME="JSON" VALUE="json"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="FORMAT_UNIPROT" FIELD="CLOSED_LIST" SHOW_IF="TRIPLESTORE_TYPE == 'UNIPROT'" NUM_ROW="15">
      <ITEMS DEFAULT="RDFXML">
        <ITEM NAME="RDFXML" VALUE="rdf"/>
        <ITEM NAME="TTL" VALUE="ttl"/>
        <ITEM NAME="NT" VALUE="nt"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="55"
      REQUIRED="true"
    >
      <DEFAULT>"CONSTRUCT {?s ?p ?o .}  
WHERE {?s ?p ?o .}"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
   
	   <PARAMETER NAME="CHECK_DISPLAY_URL" FIELD="CHECK" NUM_ROW="10">
	      <DEFAULT>false</DEFAULT>
	   </PARAMETER>

   </ADVANCED_PARAMETERS>

  <CODEGENERATION/>

  <RETURNS/>

</COMPONENT>
